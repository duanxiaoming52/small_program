/* word/pages/index/index.wxss */
.container{
  padding: 12rpx 30rpx;
  height: 100%;
}
.container .search{
  margin-top: 12rpx;
  margin-bottom:12rpx;
  position: relative;
  padding: 12rpx 100rpx 12rpx 12rpx;
  background-color: #f0f0f0;
}
.container .search input{
  background-color: #fff;
}
.container .search text{
  position: absolute;
  right: 0rpx;
  top:0rpx;
  padding: 18rpx 24rpx;
}
.container swiper{
  width: 690rpx;
  height: 690rpx;
}
.container swiper image{
  width: 100%;
  height: 100%;
}
.container .alert{
  color: #FF0000;
  font-size: 20rpx;
}